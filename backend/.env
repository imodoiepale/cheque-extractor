# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Google Vision API
VISION_API_KEY=your-google-vision-api-key-here
VISION_API_PROJECT_ID=your-project-id

# Processing Config
MAX_FILE_SIZE=10485760
SUPPORTED_FORMATS=pdf,png,jpg,jpeg
OCR_LANGUAGE=eng
CONFIDENCE_THRESHOLD_AUTO=0.90
CONFIDENCE_THRESHOLD_REVIEW=0.70

# Redis (for queue)
REDIS_URL=redis://localhost:6379

# Server
PORT=4000
NODE_ENV=development

# Logging
LOG_LEVEL=debug

# QuickBooks
QBO_CLIENT_ID=your-qbo-client-id
QBO_CLIENT_SECRET=your-qbo-client-secret
QBO_ENVIRONMENT=sandbox
QBO_REDIRECT_URI=http://localhost:3000/api/qbo/callback